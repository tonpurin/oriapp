<li class="selected-item ui-id-<%= owner_item.id %>" style="margin: 0;">
	<div class="voted-result">
		<p><%= "#{owner_item.vote_num}" %></p>
	</div>
	<div class="selected-item-info clearfix" data-id="<%= owner_item.id %>">
		<div class="item-info-left">
			<a href=<%= owner_item.item_url %> target="_blank">
				<img src=<%= owner_item.image_url %>>
			</a>
		</div>
		<div class="item-info-middle">
			<p><%= owner_item.item_name%></p>
			<p><%= owner_item.item_address%></p>
		</div>
		<div class="item-info-right">
			<%= image_tag "gurume-right.png", :class => "top-img"%>
			<%= link_to image_tag("dislike.png", :class => "bottom-img"), "/owners/#{owner_item.id}", method: :delete, :remote => true %>
		</div>
	</div>
</li>