<li class="selected-item ui-id-<%= voted_item[1].item.id %>">
	<div class="selected-item-info clearfix" data-id="<%= voted_item[1].item.id %>">
		<div class="item-info-left">
			<a href=<%= voted_item[1].item.item_url %> target="_blank">
				<img src=<%= voted_item[1].item.image_url %>>
			</a>
		</div>
		<div class="item-info-middle">
			<p><%= "得票数 : #{voted_item[0]}" %></p>
			<p><%= voted_item[1].item.item_name%></p>
			<p><%= voted_item[1].item.item_address%></p>
		</div>
		<div class="item-info-right">
			<%= image_tag "gurume-icon.png", :class => "top-img"%>
			<%= link_to image_tag("dislike.png", :class => "bottom-img"), "/owners/#{voted_item[1].id}", method: :delete, :remote => true %>
		</div>
	</div>
</li>